        <Rectangle height="20" width="125" fx:id="fourth" GridPane.columnIndex="1" GridPane.rowIndex="2" strokeWidth="0"/>
        <Rectangle height="20" width="20" fx:id="firstShort" GridPane.columnIndex="0" GridPane.rowIndex="0" strokeWidth="0"/>
        <Rectangle height="20" width="20" fx:id="secondShort" GridPane.columnIndex="0" GridPane.rowIndex="2" strokeWidth="0"/>
        <Rectangle height="20" width="20" fx:id="thirdShort" GridPane.columnIndex="2" GridPane.rowIndex="0" strokeWidth="0"/>
